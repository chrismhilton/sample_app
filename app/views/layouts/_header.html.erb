<!-- a partial for the site header -->

<!-- header element containing logo and site navigation -->
<header>
  <!-- link to logo using helper method -->
  <%= link_to logo, root_path %>
  <nav class="round">
    <!-- unordered list (ul) containing list items (li) -->
    <ul>
      <!-- Rails helper "link_to" with link text and named route -->
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "Help", help_path %></li>
      <li><%= link_to "Sign in", '#' %></li>
    </ul>
  </nav>
</header>